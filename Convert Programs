//Convert prgrms feet to meter

func convertFunction(feet:Double)->Double{
  let num = 3.281
  let meter = feet/num
  print(meter)
  return 0
}
print(convertFunction(feet:40))

////Convert prgrms farhenheit to celsius

func convertFunction2(farh:Double)->Double{
  let celsius = (farh - 32)*(5/9)
  print(celsius)
  return 0
}
print(convertFunction2(farh:78.8))

//Convert days to week ,years and months

func convertFunction5(days:Int)->Int{
  let year = days/365
  let week = (days%365)/7
  let days = days - ((year*365) + (week*7))
  print(year)
  print(week)
  print(days)
  return 0
}
print(convertFunction5(days:1329))
